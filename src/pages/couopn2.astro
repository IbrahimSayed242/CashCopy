---
import Layout from "../layouts/Layout.astro";
import About_card3 from "../components/sections/About_card3.astro";
import HeroCoupon3 from "../components/sections/HeroCoupon3.astro";
import Coupn3 from "../components/sections/Coupn3.astro";
import CouponForm from "../components/vue/CouponForm.vue";


// Canonical URL
const path = new URL(Astro.url.pathname, Astro.site).href;
---

<Layout title="CouponsCopy" path={path} description="">
    <main class="flex flex-col gap-y-20 md:gap-y-32 overflow-hidden">
      <HeroCoupon3/>     
       


<About_card3/>

<Coupn3/>


<CouponForm client:only="vue" />


   
</Layout>



